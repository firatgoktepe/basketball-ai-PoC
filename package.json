{
  "name": "basketball-quick-stats",
  "version": "0.1.0",
  "author": "Fırat Göktepe",
  "private": true,
  "license": "MIT",
  "description": "AI-powered basketball game analysis tool for extracting stats from video footage",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:eval": "tsx lib/test-utils/run-evaluation.ts",
    "test:eval:verbose": "tsx lib/test-utils/run-evaluation.ts --verbose",
    "clean": "rm -rf node_modules && rm -rf .next && rm -rf .turbo && rm -rf .cursor && rm -rf .vercel && rm -rf .env && rm -rf .env.local && rm -rf .env.development.local && rm -rf .env.test.local && rm -rf .env.production.local",
    "setup": "sh setup.sh",
    "dev:clean": "sh dev.sh",
    "dev:first": "./dev.sh",
    "postinstall": "node scripts/copy-wasm-files.js && bash scripts/download-movenet.sh",
    "prepare": "node scripts/copy-wasm-files.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tensorflow/tfjs": "^4.15.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.0.0",
    "comlink": "^4.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unicorn": "^61.0.2",
    "lucide-react": "^0.394.0",
    "next": "15.0.0",
    "onnxruntime-web": "1.23.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "tesseract.js": "^5.0.4",
    "typescript-eslint": "^8.46.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "postcss": "^8.0.0",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
